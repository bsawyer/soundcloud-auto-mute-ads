{
  "name": "no-ads-on-soundcloud",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ],
  "content_scripts": [{
    "matches": [
      "https://*.soundcloud.com/*"
    ],
    "js": [
      "content.js"
    ],
    "all_frames": false
  }]
}
